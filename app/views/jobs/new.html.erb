<%= form_with model: @jobs do |f| %>
<div>
    <%= f.label :title %>
    <%= f.text_field :title %>
</div>

<div>
    <%= f.label :description %>
    <%= f.text_field :description %>
</div>

<div>
    <%= f.label :wage %>
    <%= f.text_field :wage %>
</div>

<div>
    <%= f.label :level %>
    <%= f.text_field :level %>
</div>

<div>
    <%= f.label :requirements %>
    <%= f.text_field :requirements %>
</div>

<div>
    <%= f.label :quantity %>
    <%= f.text_field :quantity %>
</div>

<div>
    <%= f.label :date %>
    <%= f.text_field :date %>
</div>

    <%= f.submit 'Submit' %>
<% end %>